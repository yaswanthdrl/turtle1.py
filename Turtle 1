import turtle

t=turtle.Turtle()

turtle.bgcolor("pink")

t.pensize(50)

t.color("white")

t.begin_fill()

t.fillcolor("black")

t.lt(140)

t.fd(280)

t.circle(-140,200)

t.setheading(60)

t.circle(-140,200)

t.fd(180)

t.end_fill()

t.goto(0,0)

t.setheading(360)

t.lt(140)

t.fd(150)

t.color("white")

t.rt(90)

t.back(200)

t.fd(880)

t.rt(45)

t.back(100)

t.fd(100)

t.setheading(90)

t.lt(45)

t.back(100)
